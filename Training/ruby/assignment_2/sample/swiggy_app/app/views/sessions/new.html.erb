<div class="container">
	<h1>Log in</h1>
	<p><%= link_to "or create an account", new_user_path %></p>
	<div class="row">
	  <div class="col-md-6 col-md-offset-3">
	    <%= form_for(:session) do |f| %>
	      <%= f.label :phone %>
	      <br></br>
	      <%= f.text_field :phone, class: 'form-control' %>
	      <br></br>
	      <%= f.label :password %>
	      <br></br>
	      <%= f.password_field :password, class: 'form-control' %>
		  <br></br>
		  <%= link_to "Forgot Password?", login_path %>
		  <br></br>
	      <%= f.submit "Log in", class: "btn btn-primary" %>
	    <% end %>

	   
	  </div>
	</div>
</div>