
<h3>User Details</h3>
<table class="table table-bordered">
<tr>
	<th>ID</th>
	<th>Name</th>
	<th>Phone</th>
	<th>Email</th>
</tr>
<% User.all.each do |user| %>
	<tr>
      <td><%= user.id%></td>
      <td><%= user.name%></td>
      <td><%= user.phone%></td>
      <td><%= user.email%></td>
	</tr>
<% end %>
</table>

